def i(word, w):
    word.append(w)
def s(word, w):
    if not word:
        return 0
    return (1 if word[0] == w else 0) + s(word[1:], w)
def starts_with(word, prefix):
    if not word:
        return 0
    return (1 if word[0].startswith(prefix) else 0) + starts_with(word[1:], prefix)
