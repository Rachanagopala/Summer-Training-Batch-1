def is_prime(num):
    if num<=1:
        return false
    for i in range (2,int(num//2)+1):
        if num%i==0:
            return False
    return True
number=73
if is_prime(number):
    print(number,"is prime")
else:
    print(number,"is not prime")
_____________________________________________________________________

def prime_digit(num):
    prime_digit={'2','3','5','7'}
    count=0
    for d in str(num):
        if d in prime_digit:
            count += 1
    return count
num=467765
print(prime_digit(num))

