def is_prime(num):
    if num<=1:
        return False
    for i in range(2,int(num**0.5)+1):
        if num%i==0:
            return False
    return True
number=61
digits=[int(digit) for digit in str(number)]
sum_of_digits=sum(digits)
if is_prime(sum_of_digits):
    print(sum_of_digits, "is prime number.")
else:
    print(sum_of_digits, "is not prime number.")

