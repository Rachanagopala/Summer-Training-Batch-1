def bfs(a,n):
    vi=[]
    q=[n]
    while(q):
        for i in a[q[0]]:
            if(i not in q and i[0] not in vi):
                q.append(i[0])
        vi.append(q.pop(0))
        print(vi[-1])
def fun(a,x,e,c,m,l1):
    l.append(x)
    if(x==e):
        if(c<m):
            m=c
            l1=l.copy()
        l.pop()
        return m,l1
    for i in a[x]:
        if i[0] not in l:
            m,l1=fun(a,i[0],e,c+i[1],m,l1)
    l.pop()
    return m,l1
a={5:[(7,2),(3,1)],3:[(5,1),(7,2),(8,3)],7:[(5,2),(3,2),(4,6)],4:[(7,6),(8,1),(2,2)],8:[(3,3),(4,1),(2,1)],2:[(4,2),(8,1)]}
