def i(word,w):
    word.append(w)
def s(word,w):
    if not word:
        return False
    if word[0]==w:
        return True
    return s(word[1:],w)
def starts_with(word,prefix):
    if not word:
        return False
    if word[0].startswith(prefix):
        return True
    return starts_with(word[1:],prefix)
