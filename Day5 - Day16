def min_coins(c,a):
    dp=[float('inf')]*(a+1)
    dp[0]=0
    for c in arr:
        for i in range(c,a+1):
            dp[i]=min(dp[i],dp[i-c]+1)
    return dp[a]
arr=[1,3,4,5]
a=17
b=min_coins(arr,a)
print(b)
